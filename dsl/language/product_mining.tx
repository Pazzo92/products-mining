import type

Program:
	"ProductType" "=" productType = ProductType
	"Criteria" "=" "{" criteria *=Criteria
	"}"
;

Criteria:
	matches = Matches | greaterThan = GreaterThan | lessThan = LessThan
;
	
Matches:
	field = STRING "=" value = STRING|INT
;

GreaterThan:
	field = STRING ">" value = INT
;

LessThan:
	field = STRING "<" value = INT
;