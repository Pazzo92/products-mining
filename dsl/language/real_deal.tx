import sample

Program:
	"ProductType" "=" productType = ProductType
	"Criteria" "=" "{" criteria *=Criteria
	"}"
;

Criteria:
	Matches | ValueCheck
;
	
Matches:
	field = STRING "=" STRING|INT
;

ValueCheck:
	GreaterThan | LessThan
;

GreaterThan:
	field = STRING ">" value = INT
;

LessThan:
	field = STRING "<" value = INT
;